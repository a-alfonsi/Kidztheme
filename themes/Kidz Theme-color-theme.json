			"scope": "storage.type.string", 
			"settings": {
			  // "foreground": "#ff8d8d",
			  "foreground": "#ffffff",
			  // "foreground": "#F13060",
			  // "foreground": "#ff82a3",
			  "fontStyle": "italic"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
